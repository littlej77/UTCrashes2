@{

}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="AllCrashes" asp-route-county="" asp-route-pageNum="" class="btn btn-outline-secondary">Home</a>

    @foreach (string c in Model)
    {
        <a asp-controller="Home" asp-action="AllCrashes" asp-route-county="@c" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedCounty == c ? "btn-primary" : "btn-outline-secondary")">@c</a>
    }

</div>